<UserControl x:Class="ChiyoS.Draw.Komari.ResultC_PraviteQT"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChiyoS.Draw.Komari"
             mc:Ignorable="d" 
             Height="200" Width="200">
    <Grid x:Name="Gd_1" Opacity="0">
        <hc:OutlineText x:Name="Tbk_Name" Text="Name" Margin="0,160,0,0" Fill="White" StrokeThickness="0.8" Stroke="#FF008FFA" FontSize="32" FontWeight="Bold" TextAlignment="Center" FontFamily="/ChiyoS.Draw.Komari;component/#Tianshi-Yanti"/>
        <TextBlock x:Name="Tbk_Num" Text="N" Margin="153,175,10,10" Foreground="#FF008FFA" FontWeight="ExtraBold" TextAlignment="Center" FontStyle="Italic" FontSize="13" FontFamily="/ChiyoS.Draw.Komari;component/#GEETYPE-WaonProGB-Flash DB"/>
        <Border x:Name="Bd_1" CornerRadius="50" Width="164" Height="160" Margin="18,0,18,40" BorderBrush="#FF9EC8E5" BorderThickness="2" >
            <Border.Background>
                <ImageBrush ImageSource="no title_115887010-cover.jpg" Stretch="UniformToFill"/>
            </Border.Background>

        </Border>
    </Grid>
    <UserControl.Resources>
        <Storyboard x:Key="Stbd_1">
            <DoubleAnimation From="0" To="1" Storyboard.TargetProperty="Opacity" BeginTime="0:0:0.1" Duration="0:0:1"/>
        </Storyboard>
    </UserControl.Resources>
</UserControl>
